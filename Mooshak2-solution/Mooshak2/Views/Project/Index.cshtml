@model IEnumerable<Mooshak2.Models.CourseViewModel>

@{
    ViewBag.Title = "Index";
}



<div class="custom-student">
    <ul class="nav nav-tabs">
        @foreach (var x in Model.ToList())
        {
            <li>
                @Ajax.ActionLink(x.Name, "SideTabRender", "Project", new AjaxOptions() {UpdateTargetId = "test", InsertionMode = InsertionMode.Replace, HttpMethod = "GET"})
